<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/stylesheets/style.css">
    <script src="https://cdn.tailwindcss.com"></script>
</head>
<body>
 <div class="w-full h-screen bg-zinc-900 p-10 text-white">
  <a href="/" class="text-blue-600">Back to home</a>
    <h1 class="text-3xl tracking-tighter">All users</h1>
   <div class="users flex gap-3 flex-wrap">
<% users.forEach(function(user)=>{%>
    <div class="user w-72 p-4 bg-zinc-800 rounded-lg">
        <div class="w-full h-72 bg-zinc-600">
            <img class="w-full h-full object-cover overflow-hidden" src="<%= user.image%>" alt="">
        </div>
            <h3 class="text-xl mt-3 tracking-tighter"><%= user.name%>></h3>
            <h5 class="text-zinc-500 "><%= user.email%></h5>
            <div class="flex gap-3 mt-10">
                
             <a href="" class="text-zinc-600">Edit User</a>
             <a href="" class="text-red-800">delete User</a>
            
        </div>
    </div>

<% 

})%>

   

   </div>
 </div>
</body>
</html>